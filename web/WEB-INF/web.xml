<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Service</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ServletModificarProducto</servlet-name>
        <servlet-class>SERVLETS.ServletModificarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEliminarProducto</servlet-name>
        <servlet-class>SERVLETS.ServletEliminarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCobrarProducto</servlet-name>
        <servlet-class>SERVLETS.ServletCobrarProducto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Service</servlet-name>
        <url-pattern>/Service</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletModificarProducto</servlet-name>
        <url-pattern>/ServletModificarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEliminarProducto</servlet-name>
        <url-pattern>/ServletEliminarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCobrarProducto</servlet-name>
        <url-pattern>/ServletCobrarProducto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
